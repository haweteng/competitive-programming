#include <iostream>
using namespace std;
int main(){
    
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>> arr[i];
    }
    for (int i=0;i<n;i++){
        int m=(((arr[i]/5)*5)+5);
        int n= (((arr[i]/5)*5)+5)- arr[i];
        if(arr[i]>=38&&n<3){
            arr[i]= m;
        }
        cout<< arr[i]<< "\n";
        }
        
    }
